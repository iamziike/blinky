{
  "author": "iamziike",
  "manifest_version": 3,
  "name": "Blinky",
  "description": "An extension that reminds you to blink you eyes when using your browser",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "./assets/logo.png",
      "24": "./assets/logo.png",
      "32": "./assets/logo.png"
    },
    "default_popup": "./app/popup/popup.html",
    "default_title": "Blinky"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./app/content/content.tsx"]
    }
  ],
  "icons": {
    "16": "./assets/logo.png",
    "48": "./assets/logo.png",
    "128": "./assets/logo.png"
  },
  "permissions": ["storage"]
}
